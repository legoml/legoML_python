#!sh
[global]
device = gpu
force_device = False
floatX = float32
reoptimize_unpickled_function = False

[cuda]
root = /usr/local/cuda/bin   # change to <Your CUDA Installation Folder>/bin

[blas]
ldflags = -L/Applications/anaconda/lib/   # after -L, change to <Your Anaconda Installation Folder>/lib
